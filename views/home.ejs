<%- include("partials/header") -%>
<h1>Home</h1>
<p><%=data%></p>
<%post.forEach(function(element){%>
    <h1><%=element.postTitle%></h1>
    <% let data=element.postData %>
    <%  data=data.substring(0,100) %>
    <% data =data+"..." %>
    
    <% let title =  element.postTitle  %>
    <% title="/posts/"+title  %>
    <p><%= data %><a href=<%=title%>>Read More </a></p>
    

    <%});%>



 
    



